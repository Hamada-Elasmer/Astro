// Pagination Area Start 
.page-number 
    .container
        nav(aria-lable="")
            ul.pagination.justify-content-center.m-auto.pt-2.mb-5
                    li.page-item.disabled
                        a.page-link(href="#" tabindex="-1" aria-disabled="true") Previous
                    li.page-item.active
                        a.page-link(href="#" aria-current="page") 1
                    li.page-item
                        a.page-link(href="#") 2
                    li.page-item
                        a.page-link(href="#") 3
                    li.page-item
                        a.page-link(href="#") 4
                    li.page-item
                        a.page-link(href="#") Next
// Pagination Area End